<p>Some facts:<ul><%
procedure PrintFacts(i : Integer);
begin
   Print(Format('%0:d + %0:d = %1:d', [i, i+i]));
end;

var i : Integer;
for i:=1 to 5 do begin
   Print('<li>');
   PrintFacts(RandomInt(100)+1);   
   PrintLn('</li>');
end;
%></ul></p>
